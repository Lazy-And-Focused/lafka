{
  "name": "@lafka/types",
  "version": "0.0.1",
  "description": "types for lafka",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist",
    "!./**/compiler.js",
    "!./**/compiler.d.ts"
  ],
  "scripts": {
    "build": "pnpm run compile && tsc && tsc-alias && pnpm run format && pnpm run remove-compiler",
    "compile": "set NODE_ENV=rights_compile&& ts-node ./src/rights/rights.types.ts",
    "format": "prettier ./src/rights/rights.types.ts -c -w",
    "remove-compiler": "rimraf ./dist/rights/compiler.d.ts && rimraf ./dist/rights/compiler.js"
  },
  "keywords": [
    "types"
  ],
  "author": {
    "name": "Lazy-And-Focused",
    "url": "https://laf-info.netlify.app/"
  },
  "contributors": [
    {
      "name": "FOCKUSTY",
      "email": "viserd.yt@gmail.com",
      "url": "http://fockusty.netlify.app/"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "catalog:",
    "mongoose": "catalog:",
    "prettier": "catalog:",
    "rimraf": "^6.0.1",
    "ts-node": "catalog:",
    "tsc-alias": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "fbit-field": "1.4.0-alpha.1b"
  }
}
